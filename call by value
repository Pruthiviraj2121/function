#include<iostream>
using namespace std;

void swap(int x,int y)
{
    int temp;
    
    temp=x;
    x=y;
    y=temp;
    cout<<"the values after swap:"<<x<<y<<"\t";
    
    return;
}
int main()
{
    int a,b;
    cout<<"Enter the value of a,b:"<<"\n";
    cin>>a>>b;
    swap(a,b);
    return 0;
    
}
